{
  "name": "eslint-plugin-simple-import-sort",
  "version": "5.0.2",
  "license": "MIT",
  "author": "Simon Lydell",
  "repository": "lydell/eslint-plugin-simple-import-sort",
  "description": "Easy autofixable import sorting",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "import",
    "imports",
    "order",
    "sort",
    "sorter",
    "sorting"
  ],
  "scripts": {
    "doctoc": "doctoc README.md",
    "prettier": "prettier --write .",
    "eslint": "eslint . --fix",
    "eslint:examples": "eslint --rulesdir src --no-ignore --fix-dry-run --format json --report-unused-disable-directives examples --ext .js,.ts,.vue,.md",
    "test": "prettier --check . && eslint . && jest --coverage",
    "prepublishOnly": "npm test"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "2.30.0",
    "babel-eslint": "10.1.0",
    "cross-spawn": "7.0.2",
    "doctoc": "1.4.0",
    "eslint": "6.8.0",
    "eslint-config-lydell": "14.0.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-markdown": "1.0.2",
    "eslint-plugin-vue": "6.2.2",
    "jest": "25.4.0",
    "prettier": "2.0.5",
    "typescript": "3.8.3"
  },
  "peerDependencies": {
    "eslint": ">=5.0.0"
  }
}
