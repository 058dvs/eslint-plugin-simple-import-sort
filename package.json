{
  "name": "eslint-plugin-simple-import-sort",
  "version": "2.0.0",
  "license": "MIT",
  "author": "Simon Lydell",
  "repository": "lydell/eslint-plugin-simple-import-sort",
  "description": "Easy autofixable import sorting",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "import",
    "imports",
    "order",
    "sort",
    "sorter",
    "sorting"
  ],
  "scripts": {
    "eslint": "eslint .",
    "eslint:fix": "npm run eslint -- --fix",
    "eslint:examples": "eslint --rulesdir src --no-ignore --fix-dry-run --format json --report-unused-disable-directives examples",
    "prettier": "prettier --write \"*.md\"",
    "doctoc": "doctoc README.md",
    "jest": "jest",
    "coverage": "jest --coverage",
    "test": "npm run eslint && npm run coverage",
    "prepublishOnly": "npm test"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "cross-spawn": "6.0.5",
    "doctoc": "1.4.0",
    "eslint": "5.12.1",
    "eslint-config-lydell": "12.0.0",
    "eslint-plugin-import": "2.15.0",
    "eslint-plugin-jest": "22.1.3",
    "eslint-plugin-prettier": "3.0.1",
    "jest": "24.0.0",
    "prettier": "1.16.1"
  },
  "peerDependencies": {
    "eslint": ">=5.0.0"
  }
}
