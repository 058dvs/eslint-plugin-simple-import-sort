{
  "name": "eslint-plugin-simple-import-sort",
  "version": "4.0.0",
  "license": "MIT",
  "author": "Simon Lydell",
  "repository": "lydell/eslint-plugin-simple-import-sort",
  "description": "Easy autofixable import sorting",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "import",
    "imports",
    "order",
    "sort",
    "sorter",
    "sorting"
  ],
  "scripts": {
    "eslint": "eslint .",
    "eslint:fix": "npm run eslint -- --fix",
    "eslint:examples": "eslint --rulesdir src --no-ignore --fix-dry-run --format json --report-unused-disable-directives examples --ext .js,.ts,.vue,.md",
    "prettier": "prettier --write \"*.md\"",
    "doctoc": "doctoc README.md",
    "jest": "jest",
    "coverage": "jest --coverage",
    "test": "npm run eslint && npm run coverage",
    "prepublishOnly": "npm test"
  },
  "dependencies": {
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "2.1.0",
    "babel-eslint": "10.0.3",
    "cross-spawn": "7.0.0",
    "doctoc": "1.4.0",
    "eslint": "6.3.0",
    "eslint-config-lydell": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.17.0",
    "eslint-plugin-markdown": "^1.0.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-vue": "5.2.3",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "eslint": ">=5.0.0"
  }
}
